// AWS Environment
access_key = "<AWS access key>"
secret_key = "<AWS secret key>"
region     = "us-east-1"

// Availability Zones
az1 = "us-east-1a"
az2 = "us-east-1b"

// Existing Infrastructure IDs
vpc_id                     = "vpc-xxxxxxxxx"
public_subnet_az1_id       = "subnet-xxxxxxxxx"
private_subnet_az1_id      = "subnet-xxxxxxxxx"
hasync_subnet_az1_id       = "subnet-xxxxxxxxx"
hamgmt_subnet_az1_id       = "subnet-xxxxxxxxx"
public_subnet_az2_id       = "subnet-xxxxxxxxx"
private_subnet_az2_id      = "subnet-xxxxxxxxx"
hasync_subnet_az2_id       = "subnet-xxxxxxxxx"
hamgmt_subnet_az2_id       = "subnet-xxxxxxxxx"
public_security_group_id   = "sg-xxxxxxxxx"
private_security_group_id  = "sg-xxxxxxxxx"
gwlb_endpoint_service_name = "com.amazonaws.vpce.us-east-1.vpce-svc-xxxxxxxxx"

// FortiGate Configuration
keyname      = "your-aws-key-pair"
license_type = "payg"
size         = "c5.xlarge"

// FortiGate IP Addresses
activeport1  = "10.1.0.10"
activeport2  = "10.1.1.10"
activeport3  = "10.1.2.10"
activeport4  = "10.1.3.10"
passiveport1 = "10.1.10.10"
passiveport2 = "10.1.11.10"
passiveport3 = "10.1.12.10"
passiveport4 = "10.1.13.10"

// Gateway IP Addresses (usually .1 of each subnet)
activeport1_gateway  = "10.1.0.1"
activeport2_gateway  = "10.1.1.1"
activeport4_gateway  = "10.1.3.1"
passiveport1_gateway = "10.1.10.1"
passiveport2_gateway = "10.1.11.1"
passiveport4_gateway = "10.1.13.1"