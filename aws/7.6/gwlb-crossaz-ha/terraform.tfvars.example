// AWS Environment
access_key = "<AWS access key>"
secret_key = "<AWS secret key>"
region     = "us-east-1"

// Availability Zones
az1 = "us-east-1a"
az2 = "us-east-1b"

// Existing Infrastructure IDs
security_vpc_id            = "vpc-xxxxxxxxx"  // Security VPC where FortiGates are deployed
customer_vpc_id            = "vpc-yyyyyyyyy"  // Customer VPC where GWLB endpoints are located
public_subnet_az1_id       = "subnet-xxxxxxxxx"
private_subnet_az1_id      = "subnet-xxxxxxxxx"
hasync_subnet_az1_id       = "subnet-xxxxxxxxx"
hamgmt_subnet_az1_id       = "subnet-xxxxxxxxx"
public_subnet_az2_id       = "subnet-xxxxxxxxx"
private_subnet_az2_id      = "subnet-xxxxxxxxx"
hasync_subnet_az2_id       = "subnet-xxxxxxxxx"
hamgmt_subnet_az2_id       = "subnet-xxxxxxxxx"
public_security_group_id   = "sg-xxxxxxxxx"
private_security_group_id  = "sg-xxxxxxxxx"
gwlb_endpoint_az1_ip       = "10.1.1.100"
gwlb_endpoint_az2_ip       = "10.1.11.100"

// FortiGate Configuration
keyname      = "your-aws-key-pair"
license_type = "payg"
size         = "c5.xlarge"

// FortiGate IP Addresses - MUST be within subnet CIDR ranges and available
// Check your subnet CIDRs and ensure these IPs are not in use
activeport1  = "10.1.0.10"   // Must be in public_subnet_az1 CIDR
activeport2  = "10.1.1.10"   // Must be in private_subnet_az1 CIDR  
activeport3  = "10.1.2.10"   // Must be in hasync_subnet_az1 CIDR
activeport4  = "10.1.3.10"   // Must be in hamgmt_subnet_az1 CIDR
passiveport1 = "10.1.10.10"  // Must be in public_subnet_az2 CIDR
passiveport2 = "10.1.11.10"  // Must be in private_subnet_az2 CIDR
passiveport3 = "10.1.12.10"  // Must be in hasync_subnet_az2 CIDR
passiveport4 = "10.1.13.10"  // Must be in hamgmt_subnet_az2 CIDR

// Gateway IP Addresses (usually .1 of each subnet CIDR)
activeport1_gateway  = "10.1.0.1"   // Gateway of public_subnet_az1
activeport2_gateway  = "10.1.1.1"   // Gateway of private_subnet_az1
activeport4_gateway  = "10.1.3.1"   // Gateway of hamgmt_subnet_az1
passiveport1_gateway = "10.1.10.1"  // Gateway of public_subnet_az2
passiveport2_gateway = "10.1.11.1"  // Gateway of private_subnet_az2
passiveport4_gateway = "10.1.13.1"  // Gateway of hamgmt_subnet_az2